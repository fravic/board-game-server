  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types", "types"],
    "rootDir": ".",
    "noEmit": true,
    "plugins": [{ "name": "nexus/typescript-language-service" }]
  },
  "include": ["types.d.ts", "."]
}
